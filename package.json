{
  "name": "kunapot.com",
  "private": true,
  "description": "Just another blog",
  "version": "0.1.1",
  "author": "aofleejay",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\" --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx src",
